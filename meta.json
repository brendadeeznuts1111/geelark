  "logging": {
    "systems": [
      {
        "name": "application",
        "type": "structured",
        "level": "INFO",
        "format": "json",
        "retention": "30d",
        "destination": "./logs/application.log",
        "compression": "gzip"
      },
      {
        "name": "audit",
        "type": "audit",
        "level": "WARN",
        "format": "encrypted-json",
        "retention": "90d",
        "destination": "./logs/audit.log",
        "compression": "gzip",
        "immutable": true
      }
    ],
    "events": {
      "userAction": {
        "triggers": ["login", "logout", "featureToggle"],
        "level": "INFO",
        "data": ["userId", "action", "timestamp"],
        "notify": ["dashboard"]
      },
      "securityEvent": {
        "triggers": ["authFailure", "permissionDenied"],
        "level": "ERROR",
        "data": ["userId", "ip", "action", "timestamp"],
        "notify": ["dashboard", "email"]
      }
    }
  },
  "monitoring": {
    "healthChecks": [
      {
        "name": "apiHealth",
        "description": "API endpoint health check",
        "interval": 30000,
        "timeout": 5000,
        "threshold": 3,
        "action": "alert",
        "severity": "high"
      },
      {
        "name": "databaseHealth",
        "description": "Database connection health",
        "interval": 60000,
        "timeout": 10000,
        "threshold": 2,
        "action": "restart",
        "severity": "critical"
      }
    ],
    "metrics": {
      "collectionInterval": 10000,
      "retention": "7d",
      "aggregation": {
        "1m": true,
        "5m": true,
        "1h": true
      },
      "exporters": ["prometheus", "grafana"]
    },
    "alerts": {
      "levels": [
        {
          "level": "warning",
          "conditions": ["cpu > 80%", "memory > 85%"],
          "notifications": ["dashboard", "email"],
          "responseTime": "5m"
        },
        {
          "level": "critical",
          "conditions": ["serviceDown", "errorRate > 10%"],
          "notifications": ["dashboard", "email", "sms"],
          "responseTime": "1m"
        }
      ]
    }
  },
  "integrations": {
    "services": [
      {
        "name": "geelark-api",
        "type": "api",
        "endpoint": "https://api.geelark.com",
        "authentication": "api-key",
        "timeout": 5000,
        "retryPolicy": {
          "attempts": 3,
          "backoff": "exponential",
          "maxDelay": 1000
        },
        "healthCheck": "/health",
        "mockEnabled": true
      },
      {
        "name": "redis-cache",
        "type": "cache",
        "endpoint": "redis://localhost:6379",
        "authentication": "none",
        "timeout": 1000,
        "poolSize": 10,
        "healthCheck": "ping"
      }
    ]
  },
