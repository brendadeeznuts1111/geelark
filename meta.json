    ],
    "events": {
      "featureChange": {
        "triggers": [
          "flag.enabled",
          "flag.disabled"
        ],
        "level": "INFO",
        "data": [
          "flag",
          "from",
          "to",
          "timestamp",
          "source"
        ],
        "notify": [
          "dashboard",
          "slack"
        ]
      },
      "securityEvent": {
        "triggers": [
          "encryption.disabled",
          "auth.failed",
          "access.violation"
        ],
        "level": "CRITICAL",
        "data": [
          "event",
          "severity",
          "user",
          "ip",
          "timestamp"
        ],
        "notify": [
          "pagerduty",
          "sms",
          "email"
        ]
      },
      "performanceAlert": {
        "triggers": [
          "latency > 5000ms",
          "errorRate > 5%",
          "memory > 85%"
        ],
        "level": "WARN",
        "data": [
          "metric",
          "value",
          "threshold",
          "timestamp"
        ],
        "notify": [
          "dashboard",
          "email"
        ]
      }
    }
  },
