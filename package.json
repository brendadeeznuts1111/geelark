{
  "name": "geelark",
  "version": "1.0.0",
  "description": "Dev HQ - Advanced codebase analysis and automation platform built with Bun",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "dev-hq": "./dist/cli.js"
  },
  "scripts": {
    "build:dev": "bun build ./src/index.ts --outdir=./dist/dev --features=ENV_DEVELOPMENT,FEAT_EXTENDED_LOGGING,FEAT_MOCK_API --minify --sourcemap",
    "build:prod-lite": "bun build ./src/index.ts --outdir=./dist/prod-lite --features=ENV_PRODUCTION,FEAT_ENCRYPTION --minify",
    "build:prod-standard": "bun build ./src/index.ts --outdir=./dist/prod-std --features=ENV_PRODUCTION,FEAT_AUTO_HEAL,FEAT_NOTIFICATIONS,FEAT_ENCRYPTION,FEAT_BATCH_PROCESSING --minify",
    "build:prod-premium": "bun build ./src/index.ts --outdir=./dist/prod-premium --features=ENV_PRODUCTION,FEAT_PREMIUM,FEAT_AUTO_HEAL,FEAT_NOTIFICATIONS,FEAT_ENCRYPTION,FEAT_BATCH_PROCESSING,FEAT_ADVANCED_MONITORING,FEAT_VALIDATION_STRICT --minify",
    "build:android": "bun build ./src/index.ts --outdir=./dist/android --features=ENV_PRODUCTION,PLATFORM_ANDROID,FEAT_ENCRYPTION --minify",
    "build:ios": "bun build ./src/index.ts --outdir=./dist/ios --features=ENV_PRODUCTION,PLATFORM_IOS,FEAT_ENCRYPTION --minify",
    "build:web": "bun build ./src/index.ts --outdir=./dist/web --features=ENV_PRODUCTION,PLATFORM_WEB,FEAT_ENCRYPTION --minify --target=browser",
    "test:dev": "bun test --features=ENV_DEVELOPMENT,FEAT_MOCK_API",
    "test:prod": "bun test --features=ENV_PRODUCTION",
    "test:premium": "bun test --features=ENV_PRODUCTION,FEAT_PREMIUM,FEAT_ADVANCED_MONITORING",
    "start:dev": "bun run --features=ENV_DEVELOPMENT,FEAT_MOCK_API ./src/index.ts",
    "start:prod": "bun run --features=ENV_PRODUCTION ./src/index.ts",
    "start:premium": "bun run --features=ENV_PRODUCTION,FEAT_PREMIUM ./src/index.ts",
    "benchmark": "bun run --features=ENV_PRODUCTION ./src/benchmark.ts",
    "analyze": "bun build --entrypoints=./src/index.ts --outdir=./dist/analyze --features=ENV_PRODUCTION --minify --analyze",
    "build:test": "bun build --tsconfig-override configs/tsconfig.dev.json --define ENV_DEVELOPMENT:true --define FEAT_MOCK_API:true src/index.ts --outdir dist/test",
    "build:audit": "bun run scripts/build-validation.ts security-audit && bun build --tsconfig-override configs/tsconfig.audit.json --define AUDIT_MODE:true --define FEAT_PREMIUM:true --define FEAT_AUTO_HEAL:true --define FEAT_NOTIFICATIONS:true --define FEAT_ENCRYPTION:true --define FEAT_BATCH_PROCESSING:true --define FEAT_ADVANCED_MONITORING:true --define FEAT_EXTENDED_LOGGING:true --define FEAT_VALIDATION_STRICT:true --no-macros --debug src/index.ts --outdir dist/audit",
    "build:compare": "bun run build:prod-lite && bun run build:prod-standard && bun run build:prod-premium && bun run scripts/analyze-bundle.ts dist && ls -lh dist/prod-*",
    "build:analyze": "bun build --tsconfig-override configs/tsconfig.prod.json --define ENV_PRODUCTION:true --define FEAT_PREMIUM:true --analyze src/index.ts",
    "build:dce": "bun run scripts/build-validation.ts production-lite && bun build --tsconfig-override configs/tsconfig.prod.json --define ENV_PRODUCTION:true --define FEAT_ENCRYPTION:true --minify --drop=console --jsx-side-effects src/index.ts --outdir dist/dce-optimized",
    "build:pure": "bun run scripts/build-validation.ts production-standard && bun build --tsconfig-override configs/tsconfig.prod.json --define ENV_PRODUCTION:true --define FEAT_PREMIUM:true --define FEAT_AUTO_HEAL:true --define FEAT_NOTIFICATIONS:true --define FEAT_ENCRYPTION:true --define FEAT_BATCH_PROCESSING:true --minify --drop=console --jsx-side-effects src/index.ts --outdir dist/pure-optimized",
    "build:security": "bun run scripts/build-validation.ts security-audit && bun build --tsconfig-override configs/tsconfig.audit.json --define AUDIT_MODE:true --define SECURITY_VALIDATION:true --no-macros --drop=console --jsx-side-effects src/index.ts --outdir dist/security",
    "test:width": "bun run --features=ENV_DEVELOPMENT ./src/index.ts --test-width",
    "build:report": "bun run --features=ENV_PRODUCTION ./src/index.ts --analyze > build-report.txt",
    "validate:features": "bun run --features=ENV_PRODUCTION ./src/index.ts --validate-features",
    "status": "bun run src/index.ts status",
    "logs": "bun run src/index.ts logs",
    "health": "bun run src/index.ts health",
    "audit": "bun run src/index.ts audit",
    "flags": "bun run src/index.ts flags",
    "dashboard": "bun run src/index.ts dashboard",
    "generate:meta": "bun run scripts/generate-meta.ts",
    "validate:meta": "bun run scripts/validate-meta.ts",
    "update:checksum": "bun run scripts/update-checksum.ts",
    "analyze:bundle": "bun run scripts/analyze-bundle.ts",
    "test": "bun test --config tests/config/bun-test.config.ts",
    "test:watch": "bun test --config tests/config/bun-test.config.ts --watch",
    "test:coverage": "bun test --config tests/config/bun-test.config.ts --coverage",
    "test:unit": "bun test --config tests/config/bun-test.config.ts tests/unit/**/*.test.ts",
    "test:integration": "bun test --config tests/config/bun-test.config.ts tests/integration/**/*.test.ts",
    "test:e2e": "bun test --config tests/config/bun-test.config.ts tests/e2e/**/*.test.ts",
    "test:dev-hq": "bun test --config tests/config/bun-test.config.ts tests/integration/dev-hq/*.test.ts",
    "test:elimination": "bun test --config tests/config/bun-test.config.ts tests/unit/feature-elimination/*.test.ts",
    "test:types": "bun test --config tests/config/bun-test.config.ts tests/unit/type-testing/*.test.ts",
    "test:servers": "bun test --config tests/config/bun-test.config.ts tests/integration/server/*.test.ts tests/integration/api/*.test.ts",
    "test:ci": "bun test --config tests/config/bun-test.config.ts --coverage --reporter=json",
    "test:api": "bun test --config tests/config/bun-test.config.ts --user-agent \"geelark-api-tests/1.0\" tests/integration/api/",
    "test:server": "bun test --config tests/config/bun-test.config.ts --user-agent \"geelark-server-tests/1.0\" tests/integration/server/",
    "test:dev-hq-api": "bun test --config tests/config/bun-test.config.ts --user-agent \"geelark-dev-hq-tests/1.0\" tests/integration/dev-hq/",
    "test:mobile": "bun test --config tests/config/bun-test.config.ts --user-agent \"Mozilla/5.0 (iPhone; CPU iPhone OS 14_0 like Mac OS X) AppleWebKit/605.1.15\" tests/integration/api/",
    "test:desktop": "bun test --config tests/config/bun-test.config.ts --user-agent \"Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36\" tests/integration/api/",
    "test:file-io": "bun test --config tests/config/bun-test.config.ts tests/unit/utils/bun-file-io.test.ts",
    "test:cli": "bun test tests/pattern.test.ts",
    "test:cli:watch": "bun --watch test tests/pattern.test.ts",
    "test:performance": "bun test tests/performance/**/*.test.ts",
    "test:networking": "bun test tests/performance/networking/**/*.test.ts",
    "test:transpilation": "bun test tests/performance/transpilation/**/*.test.ts",
    "test:configuration": "bun test tests/performance/configuration/**/*.test.ts",
    "test:benchmark": "bun test tests/performance/benchmark-utils.test.ts",
    "bench": "bun run src/cli/benchmark-cli.ts",
    "bench:time": "bun run src/cli/benchmark-cli.ts time",
    "bench:memory": "bun run src/cli/benchmark-cli.ts memory",
    "bench:stress": "bun run src/cli/benchmark-cli.ts stress --iterations 500",
    "bench:compare": "bun run src/cli/benchmark-cli.ts compare",
    "bench:network": "bun run src/cli/benchmark-cli.ts network",
    "bench:transpilation": "bun run src/cli/benchmark-cli.ts transpilation",
    "bench:configuration": "bun run src/cli/benchmark-cli.ts configuration",
    "bench:profile": "bun --cpu-prof --cpu-prof-dir ./profiles run src/cli/benchmark-cli.ts time",
    "lint": "bun run scripts/lint.ts",
    "lint:fix": "bun run scripts/lint.ts --fix",
    "type-check": "bun run scripts/type-check.ts",
    "clean": "rm -rf dist/*",
    "clean:all": "rm -rf dist/* node_modules/.cache",
    "dev": "bun --watch src/index.ts",
    "dev:debug": "bun --inspect --watch src/index.ts",
    "bench:runtime:expose-gc": "bun --expose-gc test bench/runtime-controls.bench.ts",
    "bench:process-lifecycle": "bun test bench/process-lifecycle.bench.ts",
    "bench:features": "bun test bench/bun-runtime-features.bench.ts",
    "hq": "bun bin/dev-hq-cli.ts",
    "insights": "bun bin/dev-hq-cli.ts insights",
    "insights:table": "bun bin/dev-hq-cli.ts insights --table",
    "insights:json": "bun bin/dev-hq-cli.ts insights --json",
    "health:watch": "bun --watch bin/dev-hq-cli.ts health",
    "build:cli": "bun build bin/dev-hq-cli.ts --outfile dist/cli.js --minify --target bun",
    "dev:cli": "bun --hot --watch bin/dev-hq-cli.ts insights --table"
  },
  "keywords": [
    "bun",
    "codebase-analysis",
    "developer-tools",
    "cli",
    "automation",
    "feature-flags",
    "dashboard",
    "monitoring",
    "performance",
    "testing",
    "transpilation",
    "typescript",
    "jsx",
    "websocket",
    "http-server"
  ],
  "author": "Dev HQ",
  "license": "MIT",
  "engines": {
    "bun": ">=1.3.6"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/brendadeeznuts1111/geelark.git"
  },
  "bugs": {
    "url": "https://github.com/brendadeeznuts1111/geelark/issues"
  },
  "homepage": "https://github.com/brendadeeznuts1111/geelark#readme",
  "files": [
    "src",
    "bin",
    "docs",
    "bunfig.toml",
    "tsconfig.json",
    "schemas"
  ]
}
